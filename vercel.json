{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/a", "destination": "/a/index.html" },
    { "source": "/b", "destination": "/b/index.html" },
    { "source": "/c", "destination": "/c/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
